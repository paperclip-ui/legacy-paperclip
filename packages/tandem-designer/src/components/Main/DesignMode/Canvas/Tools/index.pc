<div export
  component
  as="Tools"
  className="tools"
  {onClick?}
  {onMouseUp?}
  {onDoubleClick?}
  {onMouseMove?}
  {onMouseLeave?}
  {onWheel?}>
  <style>
    position: absolute;
    top: 0px;
    left: 0px;
    background: transparent;
    width: 100%;
    height: 100%;
  </style>
  {children}
</div>


