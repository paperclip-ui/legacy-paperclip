<import src="components/Theme/index.pc" as="theme" />
<import src="components/Theme/open-sans/font-face.pc" as="" />


<div export component as="Frame" {style?} {className?}>
  <style>
    position: absolute;
    width: 1024px;
    height: 768px;
    :global(iframe) {
      width: 100%;
      height: 100%;
    }
  </style>
  {children}
</div>


<Frame>
  <style>
    top: 50%;
    left: 50%;
    width: 300px;
    height: 400px;
    transform: translate(-50%, -50%);
  </style>
</Frame>